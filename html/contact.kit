   <main class="main">
    <section class="contact">
        <div class="wrapper">
            <div class="contact__box">
        <h2 class="contact__title">Kontakt</h2>
<form class="contact__form">


    <label for="name" class="contact__label">Imię i nazwisko:</label>
    <input id="name" type="text" class="contact__input">
    <p class="contact__name-error error-alert">error</p>
    <label for="email" class="contact__label">E-mail:</label>
    <input id="email" class="contact__input"type="email" >
    
    <p class="contact__email-error error-alert">error</p>
    <label for="msg" class="contact__label">Wiadomość:</label>
    <textarea id="msg" cols="30" rows="10" class="contact__textarea" placeholder="Wpisz wiadomość"></textarea>
    <p class="contact__msg-error error-alert">error</p>
    <button class="contact__btn">Wyślij</button>
   
</form>    
